{
  "name": "mfe-ss",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently npm:serve:nossl:*",
    "start:ssl": "concurrently npm:serve:ssl:*",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:nossl:container": "ng serve --port 4200 -o --project container --live-reload false --disable-host-check ",
    "serve:nossl:hello": "ng serve --port 4201 --project hello --live-reload false --disable-host-check --serve-path " ,
    "serve:ssl:container": "ng serve --port 4200 -o --project container --live-reload false --disable-host-check --ssl --ssl-key localhost.key --ssl-cert localhost.crt ",
    "serve:ssl:hello": "ng serve --port 4201 --project hello --live-reload false --disable-host-check  --ssl --ssl-key localhost.key --ssl-cert localhost.crt "
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^14.0.0",
    "@angular/animations": "^14.0.2",
    "@angular/common": "^14.0.2",
    "@angular/compiler": "^14.0.2",
    "@angular/core": "^14.0.2",
    "@angular/forms": "^14.0.2",
    "@angular/platform-browser": "^14.0.2",
    "@angular/platform-browser-dynamic": "^14.0.2",
    "@angular/router": "^14.0.2",
    "@types/systemjs": "^6.1.1",
    "container": "0.0.2",
    "import-map-overrides": "^3.0.0",
    "rxjs": "~7.4.0",
    "single-spa": ">=4.0.0",
    "single-spa-angular": "^6.3.1",
    "systemjs": "^6.12.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "14.0.0",
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular/cli": "^14.0.2",
    "@angular/compiler-cli": "^14.0.2",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "style-loader": "^3.3.1",
    "typescript": "~4.7.3"
  }
}
